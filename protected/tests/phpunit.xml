<phpunit bootstrap="bootstrap.php"
		colors="false"
		convertErrorsToExceptions="true"
		convertNoticesToExceptions="true"
		convertWarningsToExceptions="true"
		stopOnFailure="false">

	<selenium>
		<browser name="Firefox" browser="*firefox" />
	</selenium>
        
        
        <filter>
            <blacklist>
                <directory suffix=".php">/var/lib/jenkins/jobs/noc2/yii</directory>
                <directory suffix=".php">/var/lib/jenkins/jobs/noc2/workspace/vendor</directory>
                <directory suffix=".php">/var/lib/jenkins/jobs/noc2/workspace/protected/config</directory>
                <directory suffix=".php">/var/lib/jenkins/jobs/noc2/workspace/protected/extensions</directory>
            </blacklist>
        </filter>

</phpunit>